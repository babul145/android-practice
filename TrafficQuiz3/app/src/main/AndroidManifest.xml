<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.firsttech.babul.trafficquiz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".LearnAboutTrafficRulesActivity" />
        <activity android:name=".AllLevelActivity" />
        <activity android:name=".LevelOneActivity" />
        <activity android:name=".LevelTwoActivity" />
        <activity android:name=".LevelThreeActivity" />
        <activity android:name=".CasesAccidentActivity"></activity>
    </application>

</manifest>
